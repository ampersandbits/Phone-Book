<% include partials/header %>
<div class="container">
    <div class="rorw">
        <h1 style="text-align:center">Add new Contact</h1>
        <div style="width:30%; margin : 0 auto;">
            <form action="/" method="POST">
                <input class="form-control" type="text" name="contact[firstname]" placeholder="First Name">
                <input class="form-control" type="text" name="contact[lastname]" placeholder="Last Name">
                <input class="form-control" type="phonenumber" name="contact[number]" placeholder="Phone Number">
                <input class="form-control" type="text" name="contact[city]" placeholder="City">
                <button class="btn btn-primary btn-block">Save</button>
            </form>
        </div>
    </div>
</div>

<% include partials/footer %>